{
  "manifest_version": 3,
  "name": "AI dost",
  "version": "0.1",
  "author": "Rishabh Jain",
  "description": "A friedn which understands you and your code and helps in debugging and solve the problem",
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://maang.in/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources" : [
    {
      "resources": ["assets/bookmark.png", "assets/delete.png"],
      "matches" : ["https://maang.in/*"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/ext-icon.png",
      "24": "assets/ext-icon.png",
      "32": "assets/ext-icon.png"
    },
    "default_popup": "popup.html",
    "default_title": "A tooltip popup description when you hover over the Chrome Extension icon."
  }
}
